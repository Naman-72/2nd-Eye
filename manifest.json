{
  "manifest_version": 3,
  "name": "URL Time Tracker",
  "version": "0.1.0",
  "description": "Tracks time spent on websites (active tab + window focused), per full URL, day-wise for 1 month.",
  "action": {
    "default_title": "Time Tracker",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
    "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
    }

}